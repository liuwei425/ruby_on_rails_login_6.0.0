<% if current_user %>
  <%= link_to 'Log Out', logout_path %>
<% else %>
  <!--  <%#= link_to 'Sign Up', signup_path %> or-->
  <%= link_to 'Log In', login_path %>
<% end %>
<% @posts.each do |post| %>
    <h1><%= link_to post.title, post %></h1>
    <p><%= post.content %></p>
<% end %>
<p><%= link_to "create", "posts/new" %></p>